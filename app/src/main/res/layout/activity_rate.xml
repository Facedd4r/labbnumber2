        android:layout_marginTop="126dp"
        android:layout_marginEnd="86dp"
        android:layout_marginBottom="422dp"
        android:text="плохо"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
